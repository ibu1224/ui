{
  "name": "screwdriver-ui",
  "version": "2.0.0",
  "description": "Ember UI for screwdriver",
  "repository": {
    "type": "git",
    "url": "git@github.com:screwdriver-cd/ui.git"
  },
  "license": "BSD-3-Clause",
  "author": "Peter Peterson <jedipetey@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "private": true,
  "scripts": {
    "build": "ember build --environment=production",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "start:dev": "sudo ember serve --ssl true --port 443 --environment=development",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "prettier --check --write",
      "git add"
    ]
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "homepage": "https://github.com/screwdriver-cd/ui",
  "bugs": "https://github.com/screwdriver-cd/ui/issues",
  "keywords": [
    "screwdriver",
    "yahoo"
  ],
